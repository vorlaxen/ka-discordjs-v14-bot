DB_TIMEZONE=+00:00
DB_SSL=false

# Redis credentials
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=mypassword
REDIS_DB=0
REDIS_TLS_ENABLED=false
REDIS_PREFIX=ka-bot:
REDIS_CONNECT_TIMEOUT=10000
REDIS_MAX_RETRIES=5
REDIS_OFFLINE_QUEUE=true
REDIS_RETRY_MAX=10