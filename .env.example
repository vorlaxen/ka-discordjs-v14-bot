# Environment
NODE_ENV=development          # development | production

# Bot Core
BOT_TOKEN=
BOT_CLIENT_SECRET=
BOT_APP_ID=
BOT_OWNER_IDS=
BOT_PREFIX=!

# Bot Logging
BOT_LOG_LEVEL=info
BOT_ERROR_CHANNEL=

# Bot Moderation
BOT_MOD_AUTO_DELETE=true
BOT_MOD_LOG_CHANNEL=

# Database credentials
DB_USERNAME=postgres
DB_PASSWORD=mypassword
DB_NAME=ka-bot
DB_HOST=localhost
